<!-- tabuleiro com a peça inicial e muitos jogadores
	Verifica Draw
		- desenhar uma única peça
	
	Verifica Play
		- escolhe uma peça válida, com ou sem meeple

	Verifica Next
		- iteração correcta de jogadores: depois do 5 (ultimo) volta ao 1

-->
<board>
	<terrain>
		<tile type="E" x="0" y="0" orientation="N"/>
	</terrain>
	<scores>
		<score player="1" score="0"/>
		<score player="2" score="0"/>
		<score player="3" score="0"/>
		<score player="4" score="0"/>
		<score player="5" score="0"/>
	</scores>
	<next tile="E"/>
</board>